<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<AnchorPane prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.meti.app.control.Menu">
    <GridPane prefHeight="768.0" prefWidth="1024.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome to Infinity!" GridPane.columnSpan="2"
              GridPane.halignment="CENTER"/>
        <Button mnemonicParsing="false" onAction="#openConnections" text="Find a Connection"
                GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="1"/>
        <Button mnemonicParsing="false" onAction="#openServers" text="Host a Server" GridPane.columnSpan="2"
                GridPane.halignment="CENTER" GridPane.rowIndex="2"/>
        <Button mnemonicParsing="false" onAction="#openLocals" text="Use Locally" GridPane.columnSpan="2"
                GridPane.halignment="CENTER" GridPane.rowIndex="3"/>
        <Button mnemonicParsing="false" onAction="#openSettings" text="Settings" GridPane.columnIndex="1"
                GridPane.halignment="RIGHT" GridPane.rowIndex="4" GridPane.valignment="BOTTOM">
            <GridPane.margin>
                <Insets bottom="25.0" right="25.0"/>
            </GridPane.margin>
        </Button>
        <Text fx:id="versionText" strokeType="OUTSIDE" strokeWidth="0.0" text="unknown" GridPane.halignment="LEFT"
              GridPane.rowIndex="4" GridPane.valignment="BOTTOM">
            <GridPane.margin>
                <Insets bottom="25.0" left="25.0"/>
            </GridPane.margin>
        </Text>
    </GridPane>
</AnchorPane>
