<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.meti.app.HostALocalServer">
    <GridPane prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <TextField fx:id="portField" GridPane.columnIndex="1" GridPane.rowIndex="1">
            <GridPane.margin>
                <Insets right="15.0"/>
            </GridPane.margin>
        </TextField>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Host A Local Server" GridPane.columnSpan="2"
              GridPane.halignment="CENTER"/>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Port:" GridPane.halignment="CENTER"
              GridPane.rowIndex="1"/>
        <Button mnemonicParsing="false" onAction="#back" text="Back" GridPane.halignment="LEFT" GridPane.rowIndex="2"
                GridPane.valignment="BOTTOM">
            <GridPane.margin>
                <Insets bottom="15.0" left="15.0"/>
            </GridPane.margin>
        </Button>
        <Button mnemonicParsing="false" onAction="#next" text="Next" GridPane.columnIndex="1"
                GridPane.halignment="RIGHT" GridPane.rowIndex="2" GridPane.valignment="BOTTOM">
            <GridPane.margin>
                <Insets bottom="15.0" right="15.0"/>
            </GridPane.margin>
        </Button>
    </GridPane>
</AnchorPane>
