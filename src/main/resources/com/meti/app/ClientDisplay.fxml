<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.meti.app.ClientDisplay">
    <BorderPane prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <top>
            <MenuBar BorderPane.alignment="CENTER">
                <Menu mnemonicParsing="false" text="File">
                    <MenuItem mnemonicParsing="false" text="Close"/>
                </Menu>
                <Menu mnemonicParsing="false" text="View">
                    <MenuItem mnemonicParsing="false" text="Action 1"/>
                </Menu>
                <Menu mnemonicParsing="false" text="Window">
                    <MenuItem mnemonicParsing="false" text="Action 1"/>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                    <MenuItem mnemonicParsing="false" text="Delete"/>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <MenuItem mnemonicParsing="false" text="About"/>
                </Menu>
            </MenuBar>
        </top>
        <center>
            <SplitPane dividerPositions="0.25" BorderPane.alignment="CENTER">
                <AnchorPane>
                    <TitledPane prefHeight="373.0" prefWidth="296.0" text="Files" AnchorPane.bottomAnchor="0.0"
                                AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                AnchorPane.topAnchor="0.0">
                        <ScrollPane fitToHeight="true" fitToWidth="true">
                            <TreeView fx:id="fileView" prefHeight="200.0" prefWidth="200.0"/>
                        </ScrollPane>
                    </TitledPane>
                </AnchorPane>
                <AnchorPane>
                    <TabPane prefHeight="373.0" prefWidth="445.0" tabClosingPolicy="UNAVAILABLE"
                             AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                             AnchorPane.topAnchor="0.0">
                        <Tab text="Chat"/>
                        <Tab text="Console">
                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"/>
                        </Tab>
                    </TabPane>
                </AnchorPane>
            </SplitPane>
        </center>
    </BorderPane>
</AnchorPane>
